    article.reservation-form.wow.fadeInUp.animated(data-wow-duration='2s')
        h2.reservation-form-title Book Your Table Online
        .container
            .row
                .col-xs-12.col-lg-10.push-lg-1
                    form(action='reservation.html', method='post')
