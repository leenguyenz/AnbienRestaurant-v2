                                input.form-control(type='text', placeholder='Your Name *')
                            .col-xs-12.col-md-6.col-lg-4.form-group
                                input.form-control(type='email', placeholder='Your Email *')
                            .col-xs-12.col-md-6.col-lg-4.form-group
                                input.form-control(type='number', placeholder='Mobile Number *')
                                i.fa.fa-mobile
                            .col-xs-12.col-md-6.col-lg-4.form-group
                                input.form-control(type='text', placeholder='Date *')
                                i.fa.fa-calendar
                            .col-xs-12.col-md-6.col-lg-4.form-group
                                input.form-control(type='text', placeholder='Time *')
                                i.fa.fa-clock-o
                            .col-xs-12.col-md-6.col-lg-4.form-group
                                input.form-control(type='text', placeholder='No. of Persons *')
                                i.fa.fa-user
                            .col-xs-12.text-xs-center
                                input.button-reservation(type='submit', value='Book Now')
